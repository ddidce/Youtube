extends layouts/main
include mixins/videoBlock

block content
    .search__header
        h3 검색결과 : #{searchingBy}
    .search__videos
        if videos.length === 0
            h5 비디오를 찾지 못했습니다
        each item in videos
            +videoBlock({
                title: item.title,
                views: item.views,
                videoFile: item.videoFile,
                id: item.id
            })